package ChoiceAndIteration;

public class ForLoops {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		for (int i=0; i < 100; i++) {
			System.out.println(i + " " + (i*i));
		}

		// expressed as "while"

		int i = 0;
		while (i < 100) { // same predicate as "for"
			System.out.println(" while " + i + " " + (i*i));
			i++;  // same as i = i + 1
		}


	}

}
