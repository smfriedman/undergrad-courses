package module2;

public class iteration {

	public static void main(String[] args) {
		
		//values from 0 to 10 inclusive
		int i = 0;
		
		while (i < 11) {
			System.out.println(i);
			i = i + 1;
		}
		
		//values from 1 to 10 inclusive
		int i2 = 1;
		
		while (i2 < 11) {
			System.out.println(i2);
			i2 = i2 + 1;
		}
		
		
	}

}
