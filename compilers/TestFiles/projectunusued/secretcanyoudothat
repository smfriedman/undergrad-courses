/* Computes factorials:  tests method calls, if, while, assignment */
public class secretcanyoudothat {

   public static int factorial(int x) {
      if (x==0) return 1;
      else return x * TestClasses.secretcanyoudothat::factorial : int (x-1);
   }
   public static void outStrln(java.lang.String s) {
     java.io.PrintStream ps;
     ps = java.lang.System::out : java.io.PrintStream;
     (ps)->println : void (s);
   }
   
   public static void main431() {
     int i;
     i = 0;
     TestClasses.secretcanyoudothat::outStrln : void ("CS431 fact test");
     while ( i < 10) {
        int gobble;
        gobble = TestClasses.secretcanyoudothat::factorial : int (i);
        i = i + 1;
     }
   }
}
